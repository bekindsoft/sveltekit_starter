		"daisyui": "^4.5.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.33",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"tailwindcss": "^3.4.0",
		"typescript": "^5.3.3",
		"vite": "^5.0.13"
	},
	"type": "module",
	"dependencies": {
		"lucia": "^3.2.0",
		"@lucia-auth/adapter-sqlite": "^3.0.1",
